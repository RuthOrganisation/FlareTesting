<?xml version="1.0" encoding="utf-8"?>
<CatapultSkin
  Version="2"
  SkinType="Component"
  Comment="HTML5 Toolbar skin"
  ComponentType="TopicToolbar"
  EnableResponsiveOutput="True"
  conditions="General.GlobalLink">
  <Stylesheet>
    <Styles>
      <Style
        Name="Toolbar">
        <Properties>
          <Property
            Name="Float">right</Property>
        </Properties>
      </Style>
      <Style
        Name="ToolbarItem">
        <Classes>
          <StyleClass
            Name="NextTopic">
            <Properties>
              <Property
                Name="BackgroundImage">url('right-link-bg.png')</Property>
              <Property
                Name="BackgroundGradient">vertical #007867 #007867</Property>
              <Property
                Name="BackgroundColor">transparent</Property>
              <Property
                Name="BorderTop">none 1px #e1e1e1</Property>
              <Property
                Name="BorderRight">none 1px #e1e1e1</Property>
              <Property
                Name="BorderBottom">none 1px #e1e1e1</Property>
              <Property
                Name="BorderLeft">none 1px #e1e1e1</Property>
              <Property
                Name="BoxShadowVShadow">0px</Property>
              <Property
                Name="Width">23px</Property>
              <Property
                Name="Height">23px</Property>
              <Property
                Name="BorderBottomLeftRadius">0px</Property>
              <Property
                Name="BorderBottomRightRadius">0px</Property>
              <Property
                Name="BorderTopRightRadius">0px</Property>
              <Property
                Name="BorderTopLeftRadius">0px</Property>
            </Properties>
          </StyleClass>
          <StyleClass
            Name="PreviousTopic">
            <Properties>
              <Property
                Name="BorderTop">none 1px #e1e1e1</Property>
              <Property
                Name="BorderRight">none 1px #e1e1e1</Property>
              <Property
                Name="BorderBottom">none 1px #e1e1e1</Property>
              <Property
                Name="BorderLeft">none 1px #e1e1e1</Property>
              <Property
                Name="BackgroundImage">url('left-link-bg.png')</Property>
              <Property
                Name="BackgroundGradient">vertical #007867 #007867</Property>
              <Property
                Name="BackgroundColor">transparent</Property>
              <Property
                Name="BoxShadowVShadow">0px</Property>
              <Property
                Name="Width">23px</Property>
              <Property
                Name="Height">23px</Property>
              <Property
                Name="BorderBottomLeftRadius">0px</Property>
              <Property
                Name="BorderBottomRightRadius">0px</Property>
              <Property
                Name="BorderTopRightRadius">0px</Property>
              <Property
                Name="BorderTopLeftRadius">0px</Property>
            </Properties>
          </StyleClass>
        </Classes>
      </Style>
    </Styles>
    <Resources>
      <Resource
        Name="right-link-bg.png"></Resource>
      <Resource
        Name="left-link-bg.png"></Resource>
    </Resources>
  </Stylesheet>
  <Strings>
    <Stylesheet
      lang="en-gb">
      <Stylesheet>
        <Styles>
          <Style
            Name="Toolbar">
            <Properties>
              <Property
                Name="Float">right</Property>
            </Properties>
          </Style>
          <Style
            Name="ToolbarItem">
            <Classes>
              <StyleClass
                Name="NextTopic">
                <Properties>
                  <Property
                    Name="Tooltip">Next</Property>
                </Properties>
              </StyleClass>
              <StyleClass
                Name="PreviousTopic">
                <Properties>
                  <Property
                    Name="Tooltip">Previous</Property>
                </Properties>
              </StyleClass>
            </Classes>
          </Style>
        </Styles>
      </Stylesheet>
    </Stylesheet>
    <Stylesheet
      lang="en-us">
      <Stylesheet>
        <Styles>
          <Style
            Name="Toolbar">
            <Properties>
              <Property
                Name="Float">right</Property>
            </Properties>
          </Style>
          <Style
            Name="ToolbarItem">
            <Classes>
              <StyleClass
                Name="NextTopic">
                <Properties>
                  <Property
                    Name="BackgroundImage">url('right-link-bg.png')</Property>
                </Properties>
              </StyleClass>
            </Classes>
          </Style>
        </Styles>
        <Resources>
          <Resource
            Name="right-link-bg.png"></Resource>
        </Resources>
      </Stylesheet>
    </Stylesheet>
  </Strings>
  <TopicToolbar
    EnableCustomLayout="true"
    Buttons="PreviousTopic|NextTopic" />
</CatapultSkin>